{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": { 
        "indent": [
            "error",
            2
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-new": 0,
        "comma-dangle": ["error", "never"]
    }
}